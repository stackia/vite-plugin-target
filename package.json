{
  "name": "vite-plugin-target",
  "version": "0.1.0",
  "description": "Make Vite support Electron, Node.js, etc.",
  "main": "index.js",
  "types": "types",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "repository": "https://github.com/vite-plugin/vite-plugin-target.git",
